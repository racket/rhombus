#lang rhombus

import:
  lib("scribble/rhombus/manual.rkt"):
    expose:
      examples
      close_eval

export:
  make_macro_eval
  close_eval

fun make_macro_eval():
  let macro_eval = manual.make_rhombus_eval()
  @examples(
    ~eval: macro_eval
    ~hidden:
      import:
        rhombus/meta open
  )
  macro_eval
