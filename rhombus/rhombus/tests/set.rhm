    ~is {1, 2, 3}
  check:
    {x, ..., 0}
    ~is {0, 1, 2, 3}
  check:
    {0, x, ...}
    ~is {0, 1, 2, 3}
  check:
    {& {10, 11}, x, ..., 0}
    ~is {0, 1, 2, 3, 10, 11}
  check:
    [{x}, ...]
    ~is [{1}, {2}, {3}]
  def [[z, ...], ...] = [[1, 2, 3], [4, 5]]
  check:
    [{z, ...}, ...]
    ~is [{1, 2, 3}, {4, 5}]

