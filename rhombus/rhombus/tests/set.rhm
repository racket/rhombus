  check orig["y"] ~is #true
  check orig["z"] ~is #false
  check m.delete(2) ~throws "element does not satisfy annotation"
  check m.delete("y") ~is #void
  check orig.delete(2) ~is #void
  check to_string(m) ~is "MutableSet{\"x\"}"

