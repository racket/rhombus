block:
  use_static
  def str = "abc"
  check:
    for List (x in str):
      x
    ~is [Char"a", Char"b", Char"c"]
  check:
    for List (x in String.to_sequence(str)):
      x
    ~is [Char"a", Char"b", Char"c"]
  check:
    for List (x in str.to_sequence()):
      x
    ~is [Char"a", Char"b", Char"c"]

