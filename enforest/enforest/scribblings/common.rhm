#lang rhombus

import:
  rhombus/scribble/manual open
  "racket.rkt" open
  meta_label:
    rhombus open
    rhombus/meta open

export:
  all_from("racket.rkt")
  shrubbery_doc
  rhombus_doc
  Rhombus
  meta_label:
    all_from(rhombus)
    all_from(rhombus/meta)

def shrubbery_doc: ModulePath 'lib("shrubbery/scribblings/shrubbery.scrbl")'
def rhombus_doc: ModulePath 'lib("rhombus/scribblings/rhombus.scrbl")'

def Rhombus: @seclink(~doc: rhombus_doc, "top"){Rhombus}
