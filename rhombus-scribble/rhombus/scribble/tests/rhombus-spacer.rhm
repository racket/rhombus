meta fun transfer_raw(new_stx, old_stx) :~ Syntax:
  fun do_multi('$new_g; ...', '$old_g; ...'):
    '$(do_group(new_g, old_g)); ...'
  fun do_group('$new_t ...' && new_g, '$old_t ...'):
    '$(do_term(new_t, old_t)) ...'.relocate_group(new_g)
  fun do_term(new_t, old_t :~ Term):
    match old_t
    | '($(old_m :: Multi))':
        let '($(new_m :: Multi))' = new_t
        '($(do_multi(new_m, old_m)))'.relocate(new_t)
    | '[$(old_m :: Multi)]':
        let '[$(new_m :: Multi)]' = new_t
        '[$(do_multi(new_m, old_m))]'.relocate(new_t)
    | '{$(old_m :: Multi)}':
        let '{$(new_m :: Multi)}' = new_t
        '{$(do_multi(new_m, old_m))}'.relocate(new_t)
    | '«'$(old_m :: Multi)'»':
        let '«'$(new_m :: Multi)'»' = new_t
        '«'$(do_multi(new_m, old_m))'»'.relocate(new_t)
    | ': $(old_m :: Multi)':
        let ': $(new_m :: Multi)' = new_t
        ': $(do_multi(new_m, old_m))'.relocate(new_t)
    | '| $(old_m :: Multi) | ...':
        let '| $(new_m :: Multi) | ...' && '| $(new_b :: Block) | ...' = new_t
        Syntax.make([#'alts, ': $(do_multi(new_m, old_m))'.relocate(new_b), ...])
          .relocate(new_t)
    | ~else:
        old_t.relocate(new_t)
  do_multi(new_stx, old_stx)

meta fun to_very_pretty_source(stx):
  fun do_concat(descs, sep):
    match descs
    | []:
        ""
    | [desc0, descn, ...]:
        PrintDesc.concat(desc0, PrintDesc.concat(sep, descn), ...)
  fun do_multi('$g; ...', ~sep = #false):
    do_concat(
      [do_group(g), ...],
      if sep
      | PrintDesc.concat(sep, PrintDesc.newline())
      | PrintDesc.newline(),
    )
  fun do_group('$t ...'):
    let ts = [t, ...]
    when ts == []
    | error("shouldn't get empty group")
    let desc_ts = [do_term(t), ...]
    let [rge, ... ~nonempty]:
      let len = ts.length()
      recur loop(prev_idx = 0, idx = 1, rges :~ List = []):
        cond
        | idx == len:
            rges.add(prev_idx..idx)
        | ts[idx] is_a Operator:
            loop(idx, idx+1, rges.add(prev_idx..idx))
        | ~else:
            loop(prev_idx, idx+1, rges)
    let [desc_ts0, ..., desc_tsn] = [desc_ts.sublist(rge), ...]
    let [desc0, ...] = [do_concat(desc_ts0, " "), ...]
    let [descn, descm, ... ~once]:
      if ts.last matches '| $_ | ...'
      | let [desc_t0, ..., desc_tn] = desc_tsn
        [do_concat([desc_t0, ...], " "), desc_tn]
      | [do_concat(desc_tsn, " ")]
    let [desc0, desc1, ...] = [desc0, ..., descn]
    PrintDesc.concat(
      desc0,
      PrintDesc.nest(
        2,
        PrintDesc.concat(PrintDesc.newline(), desc1),
      ),
      ...,
      PrintDesc.concat(PrintDesc.newline(), descm), ...,
    )
  fun do_term(t):
    fun do_open_close(open, m, close, ~sep = #false):
      match do_multi(m, ~sep)
      | "":
          if close
          | PrintDesc.concat(open, PrintDesc.newline(), close)
          | open
      | desc:
          PrintDesc.concat(
            open,
            PrintDesc.nest(
              2,
              PrintDesc.concat(PrintDesc.newline(), desc),
            ),
            if close
            | PrintDesc.concat(sep || "", PrintDesc.newline(), close)
            | "",
          )
    match t
    | '($(m :: Multi))':
        do_open_close("(", m, ")", ~sep: ",")
    | '[$(m :: Multi)]':
        do_open_close("[", m, "]", ~sep: ",")
    | '{$(m :: Multi)}':
        do_open_close("{", m, "}", ~sep: ",")
    | '«'$(m :: Multi)'»':
        PrintDesc.concat(
          "'",
          PrintDesc.align(do_open_close("«", m, "»")),
          "'",
        )
    | ': $(m :: Multi)':
        do_open_close(":", m, #false)
    | '| $(m :: Multi) | ...':
        do_concat(
          [do_open_close("|", m, #false), ...],
          PrintDesc.newline(),
        )
    | ~else:
        Printable.describe(t)
  let desc = do_multi(stx)
  let outp = Port.Output.open_string()
  Printable.render(desc, outp)
  outp.get_string()

meta fun with_very_pretty_source(stx :~ Syntax):
  let str = to_very_pretty_source(stx)
  let new_stx = shrubbery.read(Port.Input.open_string(str))
  let new_stx = transfer_raw(new_stx, stx)
  let new_str = new_stx.to_source_string()
  unless new_str == str
  | error(
      ~who: stx.srcloc()!!.to_report_string(),
      "expected very pretty source string",
      error.val(~label: "syntax object", stx),
      error.text(~label: "got", new_str),
      error.text(~label: "expected", str),
    )
  new_stx

expr.macro 'check_spacer:
              $orig
              $pat':
  ~op_stx self
  let check_id = 'check'.relocate(self)
  fun make_checks(orig :~ Group):
    let adjusted = spacer.adjust_group(orig, #false, '#,')
    '«block:
        def adjusted = Syntax.literal '$adjusted'
        $check_id:
          adjusted.to_source_string()
          ~is #%literal $(orig.to_source_string())
        $check_id:
          adjusted
          ~matches '$pat'
      »'
  'block:
     $(make_checks(orig))
     $(make_checks(with_very_pretty_source(orig)))'

fun check_typeset_space(t :~ Term, context):
  let t_space = t.property(#'#{typeset-space-name})
  let ok = t_space == context
  unless ok
  | println(
      ~out: stderr,
      error.message(
        ~who: t.srcloc()!!.to_report_string(),
        "wrong space for term",
        error.val(~label: "term", t),
        error.val(~label: "got", t_space),
        error.val(~label: "expected", context),
      ))
  ok

namespace def_unquote
defn.macro 'def_unquote $name: $context_expr':
  let name_context = Syntax.make_temp_id(name +& "_context", ~keep_name: #true)
  '«def $name_context = ($context_expr)
    syntax_class $name:
      kind ~term
    | '$('$') t':
        match_when check_typeset_space(t, $name_context)
    unquote_bind.macro '$name':
      '_ :: $name'
    »'
defn.macro 'def_unquote.full $name':
  let name_full = Syntax.make_temp_id(name +& "_full", ~keep_name: #true)
  let full:
    let name_sym = name.unwrap()
    if name_sym == #'expr
    | '#false'
    | '#'$(Symbol.from_string("rhombus/" +& name_sym))'
  'def_unquote $name: #'$name
   namespace $name:
     export:
       rename:
         $name_full as full
     def_unquote $name_full: $full'

